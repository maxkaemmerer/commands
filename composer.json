{
    "name": "maxkaemmerer/commands",
    "description": "Provides Interfaces and Implementations for Command, CommandHandler, CommandBus communication",
    "keywords": ["library", "composer", "package", "php", "command", "handler", "bus"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Kaemmerer",
            "email": "maxkaemmerer.hb@googlemail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MaxKaemmerer\\Commands\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxKaemmerer\\Commands\\Tests\\": "tests"
        }
    }
}
